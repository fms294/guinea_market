{
  "expo": {
    "name": "Dibida",
    "slug": "guinea_market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.guineamarket.dibida",
      "buildNumber": "1.0.10",
      "supportsTablet": true
    },
    "android": {
      "package": "com.guineamarket.dibida",
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFF"
      },
      "googleServicesFile": "./app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
